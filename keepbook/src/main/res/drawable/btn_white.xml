<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="0dp" android:topRightRadius="0dp" />
            <solid android:color="@color/bt_light_gray" />
            <padding android:bottom="0dp" android:left="0dp" android:right="0dp" android:top="0dp" />
            <size android:width="40dp" android:height="60dp" />
            <stroke android:width="0dp" android:color="@color/bt_light_gray_more" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="0dp" android:topRightRadius="0dp" />
            <solid android:color="@color/bt_light_gray" />
            <padding android:bottom="0dp" android:left="0dp" android:right="0dp" android:top="0dp" />
            <size android:width="40dp" android:height="60dp" />
            <stroke android:width="0dp" android:color="@color/bt_light_gray_more" />
        </shape>
    </item>
    <item android:drawable="@drawable/btn_transparency_n" />
</selector>
